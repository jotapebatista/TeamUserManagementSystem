@model TeamUserManagementSystem.Models.Team

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Team</h4>
    <dl class="row">
        <dt class="col-sm-2">Team Name</dt>
        <dd class="col-sm-10">@Html.DisplayFor(model => model.TeamName)</dd>
